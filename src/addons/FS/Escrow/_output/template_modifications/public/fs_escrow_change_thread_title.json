{
    "template": "forum_post_thread",
    "description": "fs_escrow_change_thread_title",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": "<xf:title>{{ phrase('post_thread') }}</xf:title>",
    "replace": "<xf:if is=\"$forum.node_id == $xf.options.fs_escrow_applicable_forum\">\n<xf:title>{{ phrase('fs_escrow_title') }}</xf:title>\n\t<xf:else />\n\t$0\n</xf:if>"
}