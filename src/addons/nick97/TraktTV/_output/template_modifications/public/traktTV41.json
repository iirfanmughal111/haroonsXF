{
    "template": "forum_view",
    "description": "Removes forum description from default display",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": "<xf:description>{$forum.Node.description|raw}</xf:description>",
    "replace": "<xf:if is=\"$forum.TVForum is empty\">\n\t$0\n</xf:if>"
}