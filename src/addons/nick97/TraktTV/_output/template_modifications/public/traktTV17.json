{
    "template": "node_list_forum",
    "description": "Changes node icon to TV poster (template: trakt_tv_node_poster)",
    "execution_order": 10,
    "enabled": true,
    "action": "preg_replace",
    "find": "/<span class=\"node-icon\" aria-hidden=\"true\">(.*?)<\\/span>/s",
    "replace": "<xf:if is=\"$node.TVForum is not empty\">\n\t<xf:include template=\"trakt_tv_node_poster\" />\n<xf:else />\n\t$0\n</xf:if>"
}